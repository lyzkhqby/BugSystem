// CRUD SQL语句
var project = {
    insert:'INSERT INTO user(id, proName, createTime, createUser) VALUES(0, ?, ?)',
    update:'update project set proName=? where id=?',
    delete: 'delete from project where id=?',
    queryById: 'select * from project where id=?',
    queryAll: 'select * from project',
    queryByNamePwd: 'select * from project where proName=?'
};

